{
    "name": "stcer/aint-queue",
    "description": "A async-queue library built on top of swoole, flexable multi-consumer, coroutine supported.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "littlesqx",
            "email": "littlesqx@gmail.com"
        },
        {
            "name": "stcer",
            "email": "ab12cxyh@163.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-swoole": ">=4.4",
        "predis/predis": "^1.1",
        "psr/log": "^1.1",
        "symfony/console": "^4.4 | ^5.0",
        "illuminate/pipeline": "^6.16",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Littlesqx\\AintQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "suggest": {
        "php": ">=7.3",
        "jeremeamia/superclosure": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "swoole/ide-helper": "@dev"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "bin": [
        "bin/aint-queue"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "archive": {
        "exclude": ["/example/"]
    }
}
